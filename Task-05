public class Main
{
    public static void main(String[] args)
    {
        int[][] myarray={{1,2,3},{3,5,8},{2,12,9}};
        int sum=myarray[0][0]+myarray[1][1]+myarray[2][2];
        System.out.println("The sum is :"+sum);
    }
}
